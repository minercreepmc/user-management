{
  "name": "user-management",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./config/mikroorm/mikrorm.config.ts"
    ]
  },
  "scripts": {
    "up": "docker compose -f ./docker-compose.dev.yml up -d",
    "down": "docker compose -f ./docker-compose.dev.yml down",
    "app": "docker logs user-management --follow",
    "app-run": "docker exec -it user-management npm run",
    "generate-user-management-client": "openapi-generator-cli generate -i ./config/swagger/swagger.json -g typescript-fetch -o ./api/user-management-client",
    "check-circular": "madge --circular src",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down",
    "migration:pending": "npx mikro-orm migration:pending",
    "schema:drop": "npx mikro-orm schema:drop --dump",
    "e2e-test": "docker compose -f docker-compose.e2e.yml up --abort-on-container-exit",
    "integration-test": "docker compose -f docker-compose.integration.yml up --abort-on-container-exit",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.7.11",
    "@mikro-orm/core": "^5.7.11",
    "@mikro-orm/migrations": "^5.7.11",
    "@mikro-orm/nestjs": "^5.1.8",
    "@mikro-orm/postgresql": "^5.7.11",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.1",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "common-base-classes": "^0.21.16",
    "jest-extended": "^3.2.4",
    "nest-typeorm-common-classes": "^0.0.22",
    "nestjs-mediator": "^0.0.5",
    "oxide.ts": "^1.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "protobufjs": "^7.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-html-reporter": "^3.9.0",
    "jest-mock-extended": "^3.0.4",
    "madge": "^6.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.148.2",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
